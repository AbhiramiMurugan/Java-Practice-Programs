public class Swappingwithout3rdelement {
public static void main(String[] args) {
	 int x = 11;
	  int y = 22;

	  System.out.println("Before Swapping");
	  System.out.println("Value of x is :" + x);
	  System.out.println("Value of y is :" + y);

	  x = x + y;
	  y = x - y;
	  x = x - y;

	  System.out.println("Before Swapping");
	  System.out.println("Value of x is :" + x);
	  System.out.println("Value of y is :" + y);
}
}
